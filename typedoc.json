{
    "$schema": "https://typedoc.org/schema.json",
    "cacheBust": false,
    "entryPoints": [
        "src/GamepadEmulator.ts",
        "src/GamepadDisplay.ts",
        "src/GamepadApiWrapper.ts",
        "src/enums.ts",
        "src/utilities.ts"
    ],
    "customCss": "./typedoc-theme.css",
    "readme": "README.md",
    "lang": "en",
    "highlightLanguages": [
        "bash",
        "css",
        "html",
        "javascript",
        "json",
        "jsonc",
        "perl",
        "ts",
        "tsx",
        "typescript"
    ],
    "searchInComments": true,
    "out": "docs",
    "darkHighlightTheme": "dark-plus",
    "lightHighlightTheme": "light-plus",
}
